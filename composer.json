{
    "name": "mickadoo/duolingo-events",
    "description": "PHP API Wrapper for the Duolingo Events API",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "ext-intl": "*",
        "guzzlehttp/guzzle": "^7.2",
        "symfony/serializer": "^5.1",
        "symfony/property-access": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Mickadoo\\DuolingoEvents\\": "src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Devery",
            "email": "michaeldevery@gmail.com"
        }
    ]
}
