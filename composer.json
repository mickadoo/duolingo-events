{
    "name": "mickadoo/duolingo",
    "description": "PHP API Wrapper for the Duolingo API",
    "type": "library",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2",
        "symfony/serializer": "^5.1",
        "symfony/property-access": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Mickadoo\\Duolingo\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test"
        }
    },
